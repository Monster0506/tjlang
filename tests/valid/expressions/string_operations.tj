# String Operations

def string_concat() -> str {
    first: str = "Hello"
    last: str = "World"
    return first + " " + last
}

def f_strings() -> str {
    name: str = "TJLang"
    version: float = 1.0
    return f"Welcome to {name} v{version}!"
}

def nested_f_strings() -> str {
    inner: str = f"inner: {42}"
    return f"outer: {inner}"
}

def string_methods() -> str {
    text: str = "  hello world  "
    trimmed: str = text.strip()
    upper: str = trimmed.upper()
    return upper
}

def string_indexing() -> str {
    text: str = "TJLang"
    first: str = text[0]
    last: str = text[-1]
    return f"{first}{last}"
}

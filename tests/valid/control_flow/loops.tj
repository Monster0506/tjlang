# Loops

def for_range() -> int {
    sum: int = 0
    for (i: int | 1..=10) {
        sum = sum + i
    }
    return sum
}

def for_array() -> int {
    numbers: [int] = [1, 2, 3, 4, 5]
    sum: int = 0
    for (n: int | numbers) {
        sum = sum + n
    }
    return sum
}

def while_loop() -> int {
    count: int = 0
    while count < 5 {
        count = count + 1
    }
    return count
}

def nested_loops() -> int {
    total: int = 0
    for (i: int | 1..=3) {
        for (j: int | 1..=3) {
            total = total + i * j
        }
    }
    return total
}

def break_continue() -> int {
    sum: int = 0
    for (i: int | 1..=10) {
        if i == 3 {
            continue
        }
        if i == 8 {
            break
        }
        sum = sum + i
    }
    return sum
}

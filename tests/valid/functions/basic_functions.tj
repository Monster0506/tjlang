# Basic Function Definitions

def add(a: int, b: int) -> int {
    return a + b
}

def greet(name: str) -> str {
    return f"Hello, {name}!"
}

def is_even(n: int) -> bool {
    return n % 2 == 0
}

def max(a: int, b: int) -> int {
    if a > b {
        return a
    } else {
        return b
    }
}

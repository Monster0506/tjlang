# Primitive Types

def test_int() -> int {
    positive: int = 42
    negative: int = -17
    zero: int = 0
    return positive + negative + zero
}

def test_float() -> float {
    pi: float = 3.14159
    e: float = 2.71828
    return pi + e
}

def test_bool() -> bool {
    true_val: bool = true
    false_val: bool = false
    return true_val and not false_val
}

def test_str() -> str {
    empty: str = ""
    hello: str = "Hello, World!"
    return empty + hello
}

def test_any() -> any {
    int_val: any = 42
    str_val: any = "hello"
    bool_val: any = true
    return int_val
}
